@article{vese_multiphase_2002,
	title = {A Multiphase Level Set Framework for Image Segmentation Using the Mumford and Shah Model},
	volume = {50},
	issn = {0920-5691, 1573-1405},
	url = {http://link.springer.com/article/10.1023/A%3A1020874308076},
	doi = {10.1023/A:1020874308076},
	abstract = {We propose a new multiphase level set framework for image segmentation using the Mumford and Shah model, for piecewise constant and piecewise smooth optimal approximations. The proposed method is also a generalization of an active contour model without edges based 2-phase segmentation, developed by the authors earlier in T. Chan and L. Vese (1999. In Scale-Space'99, M. Nilsen et al. (Eds.), {LNCS}, vol. 1682, pp. 141{\textendash}151) and T. Chan and L. Vese (2001. {IEEE-IP}, 10(2):266{\textendash}277). The multiphase level set formulation is new and of interest on its own: by construction, it automatically avoids the problems of vacuum and overlap; it needs only log n level set functions for n phases in the piecewise constant case; it can represent boundaries with complex topologies, including triple junctions; in the piecewise smooth case, only two level set functions formally suffice to represent any partition, based on The Four-Color Theorem. Finally, we validate the proposed models by numerical results for signal and image denoising and segmentation, implemented using the Osher and Sethian level set method.},
	language = {en},
	number = {3},
	urldate = {2013-07-09},
	journal = {International Journal of Computer Vision},
	author = {Vese, Luminita A. and Chan, Tony F.},
	month = dec,
	year = {2002},
	keywords = {Active contours, Artificial Intelligence (incl. Robotics), Automation and Robotics, Computer Imaging, Graphics and Computer Vision, curvature, denoising, edge detection, energy minimization, image processing, Image segmentation, level sets, multi-phase motion, {PDE's}},
	pages = {271--293}
},

@article{xu_snakes_1998,
	title = {Snakes, shapes, and gradient vector flow},
	volume = {7},
	issn = {1057-7149},
	doi = {10.1109/83.661186},
	abstract = {Snakes, or active contours, are used extensively in computer vision and image processing applications, particularly to locate object boundaries. Problems associated with initialization and poor convergence to boundary concavities, however, have limited their utility. This paper presents a new external force for active contours, largely solving both problems. This external force, which we call gradient vector flow ({GVF)}, is computed as a diffusion of the gradient vectors of a gray-level or binary edge map derived from the image. It differs fundamentally from traditional snake external forces in that it cannot be written as the negative gradient of a potential function, and the corresponding snake is formulated directly from a force balance condition rather than a variational formulation. Using several two-dimensional (2-D) examples and one three-dimensional (3-D) example, we show that {GVF} has a large capture range and is able to move snakes into boundary concavities},
	number = {3},
	journal = {{IEEE} Transactions on Image Processing},
	author = {Xu, Chenyang and Prince, {J.L.}},
	year = {1998},
	keywords = {active contour, Active contours, Application software, binary edge map, boundary concavities, computer vision, computer vision applications, convergence, convergence of numerical methods, Deformable models, edge detection, external force, force balance condition, gradient vector flow, gradient vectors diffusion, gray-level map, Image edge detection, image processing, image processing applications, image representation, Image segmentation, image shape representation, initialization, large capture range, linear differential equations, linear partial differential equations, minimisation, minimization, object boundaries location, partial differential equations, Shape, snakes, Two dimensional displays},
	pages = {359--369}
},

